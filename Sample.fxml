<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="541.0" prefWidth="633.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="SampleController">
    <stylesheets>
        <URL value="@application.css" />
    </stylesheets>
   <children>
      <Label layoutX="330.0" layoutY="70.0" prefHeight="49.0" prefWidth="221.0" text="LoginPage">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
      <AnchorPane prefHeight="541.0" prefWidth="230.0" style="-fx-background-color: black;" />
      <Label layoutX="264.0" layoutY="185.0" text="Username">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Label layoutX="272.0" layoutY="262.0" prefHeight="40.0" prefWidth="139.0" text="Password">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <TextField fx:id="tfUser" layoutX="402.0" layoutY="193.0" prefHeight="25.0" prefWidth="174.0">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <PasswordField fx:id="tfPass" layoutX="408.0" layoutY="270.0" prefHeight="31.0" prefWidth="174.0">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
      <Button fx:id="btnLogin" layoutX="402.0" layoutY="342.0" mnemonicParsing="false" onAction="#onLogin" text="Login">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Label layoutX="313.0" layoutY="470.0" text="Not a member">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button fx:id="btnSign" layoutX="470.0" layoutY="446.0" mnemonicParsing="false" onAction="#onSignUp" prefHeight="63.0" prefWidth="118.0" text="SignUp">
         <font>
            <Font size="26.0" />
         </font>
      </Button>
      <Label fx:id="message" layoutX="348.0" layoutY="118.0" prefHeight="31.0" prefWidth="174.0" />
   </children>
</AnchorPane>
